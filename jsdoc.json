{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "exculdePattern": "(node_modules/|docs)"
  },
  "sourceType": "module",
  "plugins": ["plugins/markdown", "./jsdoc-ignore-imports.js"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/"
  }
}
